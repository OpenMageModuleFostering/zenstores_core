<config>
    <modules>
        <Zenstores_Core>
            <version>1.0.0</version>
        </Zenstores_Core>
    </modules>
    <global>
        <helpers>
            <zenstores_core>
                <class>Zenstores_Core_Helper</class>
            </zenstores_core>
        </helpers>
        <resources>
            <zenstores_core_setup>
                <setup>
                    <module>Zenstores_Core</module>
                </setup>
            </zenstores_core_setup>
        </resources>
        <blocks>
            <resources>
                <class>Zenstores_Core_Block</class>
            </resources>
        </blocks>
    </global>
    <admin>
        <routers>
            <zenstores_core_admin>
                <use>admin</use>
                <args>
                    <module>Zenstores_Core</module>
                    <frontName>zenstores_core_admin</frontName>
                </args>
            </zenstores_core_admin>
        </routers>
    </admin>
</config>
